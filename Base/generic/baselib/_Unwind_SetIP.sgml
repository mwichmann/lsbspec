<refsynopsisdiv>
<funcsynopsis>
<funcsynopsisinfo>
#include &lt;unwind.h&gt;
</funcsynopsisinfo><funcprototype>
<funcdef>void
<function>_Unwind_SetIP</function>
</funcdef>
<paramdef>struct _Unwind_Context *
