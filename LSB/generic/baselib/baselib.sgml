<FUNCSYNOPSIS>
<FUNCSYNOPSISINFO>
#include &lt;rpc/pmap_clnt.h&gt;
</FUNCSYNOPSISINFO>
<FUNCPROTOTYPE>
 <FUNCDEF>bool_t
  <FUNCTION>pmap_unset</FUNCTION></FUNCDEF>
 <PARAMDEF>u_long
