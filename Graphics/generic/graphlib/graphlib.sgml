 Bool signb = XSyncValueIsNegative(b);\
 ((presult)->lo = (a).lo - (b).lo);\
 ((presult)->hi = (a).hi - (b).hi);\
 if (t>(presult)->lo) (presult)->hi--;\
 *poverflow = ((signa == signb) &amp;&amp; !(signa == XSyncValueIsNegative(*presult)));\
}

