<refsynopsisdiv>
<funcsynopsis>
<funcsynopsisinfo>
#include &lt;png.h&gt;
</funcsynopsisinfo><funcprototype>
<funcdef>png_uint_32
<function>png_get_iCCP</function>
</funcdef>
<paramdef>png_structp
<parameter>png_ptr</parameter>
</paramdef>
