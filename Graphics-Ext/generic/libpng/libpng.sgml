</funcdef>
<paramdef>png_structp
<parameter>png_ptr</parameter>
</paramdef>
<paramdef>png_voidp
<parameter>io_ptr</parameter>
</paramdef>
<paramdef>png_rw_ptr
<parameter>write_data_fn</parameter>
</paramdef>
<paramdef>png_flush_ptr
<parameter>output_flush_fn</parameter>
</paramdef>

</funcprototype>
