#include &lt;png.h&gt;
</funcsynopsisinfo><funcprototype>
<funcdef>png_uint_32
<function>png_get_valid</function>
</funcdef>
<paramdef>png_structp
<parameter>png_ptr</parameter>
</paramdef>
